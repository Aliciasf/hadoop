        <archive-command>-fs NAMENODE -archiveName dir0.har /dir0/ /dest</archive-command>
        <command>-fs NAMENODE -rm har:///dest/dir0.har/dir0/file0</command>
      </test-commands>
      <cleanup-commands>
        <command>-fs NAMENODE -rmr /*</command>
